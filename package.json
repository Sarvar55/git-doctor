{
	"name": "git-doctor",
	"version": "0.0.0",
	"description": "GitDoctor, a TypeScript-powered command-line marvel, revolutionizes your Git experience with a range of unique features tailored for efficient version control.",
	"main": "src/index.ts",
	"repository": {
		"url": "https://github.com/Sarvar55/git-doctor"
	},
	"keywords": [],
	"scripts": {
		"start": "node build/index.js",
		"build": "tsc",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"type": "module",
	"release": {
		"branches": [
			"master"
		]
	},
	"author": "Sarvar55",
	"license": "ISC",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"eslint": "^8.56.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "3.2.5",
		"typescript": "^5.3.3"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
